<%= @list.name %>

<%= link_to 'Index', root_path  %>
<% @list.movies.each do |movie| %>
  <li><%= movie.title %></li>
<% end %>

<%= link_to 'Back', root_path %>
<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.input :comment %>
  <%= f.association :movie %>
  <%= f.button :submit %>
<% end %>
